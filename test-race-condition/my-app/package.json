    "@sveltejs/vite-plugin-svelte": "6.2.0",
    "@types/chroma-js": "3.1.1",
    "autoprefixer": "10.4.21",
    "bits-ui": "2.10.0",
    "clsx": "2.1.1",
    "lucide-svelte": "0.544.0",
